{
    "data" : {
        "structure" : {
            "values" : {
                "fields" : [ 
                    {
                        "id" : "${source}"
                    },
                    {
                        "id" : "${target}"
                    },
                    {
                        "id" : "${sourceGroup}"
                    },
                    {
                        "id" : "${targetGroup}"
                    },
                    {
                        "id" : "${count}"
                    } 
                ],
                "limit" : {
                    "enabled" : "true",
                    "size" : "${limit}"
                }
            }
        }
    },
    "tabs" : [ 
        {
            "name" : "Data",
            "controls" : [ 
                {
                    "id" : "source",
                    "type" : "field",
                    "label" : "Child"
                },
                {
                    "id" : "target",
                    "type" : "field",
                    "label" : "Parent"
                },
                {
                    "id" : "sourceGroup",
                    "type" : "field",
                    "label" : "Child Group (Optional)"
                },
                {
                    "id" : "targetGroup",
                    "type" : "field",
                    "label" : "Parent Group (Optional)"
                },
                {
                    "id" : "count",
                    "type" : "field",
                    "label" : "Count (Optional)"
                } 
            ]
        },
        {
            "name" : "Tree",
            "controls" : [ 
                {
                    "id" : "maxNodes",
                    "type" : "number",
                    "label" : "Max Nodes",
                    "defaultValue" : "10000",
                    "min" : "1",
                    "max" : "100000"
                },
                {
                    "id" : "animate",
                    "type" : "boolean",
                    "label" : "Animate",
                    "defaultValue" : "false"
                },
                {
                    "id" : "expandGroups",
                    "type" : "boolean",
                    "label" : "Expand Groups",
                    "defaultValue" : "false"
                }   
            ]
        } 
    ]
}
