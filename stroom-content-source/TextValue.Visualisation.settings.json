{
    "tabs" : [ 
        {
            "name" : "Data",
            "controls" : [ 
                {
                    "id" : "field",
                    "type" : "field",
                    "label" : "Field"
                },
                {
                    "id" : "gridSeries",
                    "type" : "field",
                    "label" : "Grid Series"
                }
            ]
        },
        {
            "name" : "Text Value",
            "controls" : [
                {
                    "id": "maxGridSeries",
                    "type": "number",
                    "label": "Max Grid Series",
                    "defaultValue": "64",
                    "min": "1",
                    "max": "128"
                }
            ]
        }
    ],
    "data" : {
        "structure" : {
            "nest": {
                "key": {
                    "id": "${gridSeries}",
                    "sort": {
                        "direction": "${sortGridSeries}",
                        "enabled": "true",
                        "priority": 0
                    }
                },
                "values" : {
                    "fields" : [ 
                        {
                            "id" : "${field}",
                            "sort": {
                                "direction": "Ascending",
                                "enabled": "true",
                                "priority": 1
                            }
                        } 
                    ],
                    "limit": {
                        "enabled": "true",
                        "size": "1"
                    }
                },
                "limit": {
                    "enabled": "true",
                    "size": "${maxGridSeries}"
                }
            }
        }
    }
}
