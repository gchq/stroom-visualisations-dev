{
    "tabs": [
      {
        "name": "Data",
        "controls": [
          {
            "id": "name",
            "type": "field",
            "label": "Name"
          },
          {
            "id": "group",
            "type": "field",
            "label": "Building Group or Campus"
          },
          {
            "id": "building",
            "type": "field",
            "label": "Building"
          },
          {
            "id": "floor",
            "type": "field",
            "label": "Floor"
          },
          {
            "id": "xPos",
            "type": "field",
            "label": "X position"
          },
          {
            "id": "yPos",
            "type": "field",
            "label": "Y position"
          },
          {
            "id": "iconName",
            "type": "field",
            "label": "Icon Name"
          },
          {
            "id": "series",
            "type": "field",
            "label": "Series"
          },
          {
            "id": "gridSeries",
            "type": "field",
            "label": "Grid Series"
          }
        ]
      },
      {
        "name": "FloorMap",
        "controls": [
          {
            "id": "isEditZoneModeEnabled",
            "type": "selection",
            "label": "Enable Zone Editing",
            "values": [
                "True",
                "False"
            ],
            "defaultValue": "False"
          },
          {
            "id": "sortSeries",
            "type": "selection",
            "label": "Sort Series",
            "values": [
              "",
              "Ascending",
              "Descending"
            ],
            "defaultValue": "Ascending"
          },
          {
            "id": "sortGridSeries",
            "type": "selection",
            "label": "Sort Grid Series",
            "values": [
              "",
              "Ascending",
              "Descending"
            ],
            "defaultValue": "Ascending"
          }
        ]
      },
      {
        "name": "Internal",
        "controls": [
          {
            "id": "config",
            "type": "text",
            "label": "FloorMap Config JSON",
            "defaultValue": ""
          }
        ]
      }
    ],
    "data": {
      "structure": {
        "nest": {
          "key": {
            "id": "${gridSeries}",
            "sort": {
              "direction": "${sortGridSeries}",
              "enabled": "true",
              "priority": 0
            }
          },
          "nest": {
            "key": {
              "id": "${series}",
              "sort": {
                "direction": "${sortSeries}",
                "enabled": "true",
                "priority": 1
              }
            },
            "values": {
              "fields": [
                {
                  "id": "${name}",
                  "sort": {
                    "direction": "Ascending",
                    "enabled": "true",
                    "priority": 2
                  }
                },
                {
                  "id": "${group}"
                },
                {
                  "id": "${building}"
                },
                {
                  "id": "${floor}"
                },
                {
                  "id": "${xPos}"
                },
                {
                  "id": "${yPos}"
                },
                {
                  "id": "${iconName}"
                },
                {
                  "id": "${series}"
                }
              ],
              "limit": {
                "enabled": "true",
                "size": "${maxValues}"
              }
            },
            "limit": {
              "enabled": "true",
              "size": "${maxSeries}"
            }
          },
          "limit": {
            "enabled": "true",
            "size": "${maxGridSeries}"
          }
        }
      }
    }
  }