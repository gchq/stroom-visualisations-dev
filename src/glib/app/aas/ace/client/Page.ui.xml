<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:v="urn:import:glib.app.aas.ace.client">
  <g:DockLayoutPanel unit="PX" width="100%" height="100%">
    <g:north size="30">
      <g:FlowPanel>
        <g:ListBox ui:field="type" />
        <g:Button ui:field="resize">Resize</g:Button>
        <g:CheckBox ui:field="autoResize">Auto Resize</g:CheckBox>
        <g:Button ui:field="update">Update</g:Button>
        <g:CheckBox ui:field="autoUpdate">Auto Update</g:CheckBox>
      </g:FlowPanel>
    </g:north>
    <g:center>
      <g:ResizeLayoutPanel ui:field="visContainer" width="100%" height="100%" />
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder> 