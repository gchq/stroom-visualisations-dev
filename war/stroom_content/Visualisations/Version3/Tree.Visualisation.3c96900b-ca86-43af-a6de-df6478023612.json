{
    "tabs": [{
        "name": "Data",
        "controls": [{
            "id": "path",
            "type": "field",
            "label": "Path"
          },
          {
            "id": "color",
            "type": "field",
            "label": "Leaf Colour Field (Optional)"
          },
          {
            "id": "gridSeries",
            "type": "field",
            "label": "Grid Series (Optional)"
          }
        ]
      },
      {
        "name": "Tree",
        "controls": [{
            "id": "delimiter",
            "type": "text",
            "label": "Path delimiter",
            "defaultValue": "/"
  
          },
          {
            "id": "showLabels",
            "type": "selection",
            "label": "Show Labels",
            "values": [
              "True",
              "False"
            ],
            "defaultValue": "True"
          },
          {
            "id": "baseColor",
            "type": "text",
            "label": "Base colour"
          },
          {
            "id": "maxValues",
            "type": "number",
            "label": "Max Values",
            "defaultValue": "500",
            "max": "10000",
            "min": "1"
          },
          {
            "id": "maxGridSeries",
            "type": "number",
            "label": "Max Grid Series",
            "defaultValue": "36",
            "min": "1",
            "max": "144"
          }
  
        ]
      }
    ],
    "data": {
      "structure": {
        "nest": {
          "key": {
            "id": "${gridSeries}",
            "sort": {
              "enabled": "false"
            }
          },
          "nest": {
            "key": {
              "id": "${series}",
              "sort": {
                "enabled": "false"
              }
            },
            "values": {
              "fields": [{
                  "id": "${path}"
                },
                {
                  "id": "${value}"
                },
                {
                  "id": "${color}"
                }
              ],
              "limit": {
                "enabled": "true",
                "size": "${maxValues}"
              }
            },
            "limit": {
              "enabled": "true",
              "size": "1"
            }
          },
          "limit": {
            "enabled": "true",
            "size": "${maxGridSeries}"
          }
        }
      }
    }
  }
  